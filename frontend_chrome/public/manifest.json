{
  "manifest_version": 3,
  "name": "Worksnap",
  "description": "Base Level Extension",
  "version": "1.0.1",
  "key": "",
  "action": {
    "default_icon": "/w_extension.png"
  },
  "permissions": [
    "tabs",
    "windows",
    "activeTab",
    "scripting",
    "identity",
    "notifications",
    "alarms",
    "storage"
  ],
  "host_permissions": ["*://*/"],
  "web_accessible_resources": [
    {
      "resources": ["public/game-alarm.wav"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "oauth2": {
    "client_id": "217103780375-1mqagu0bod0a1u228td60g566qm4ivo8.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/contacts.readonly",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
